{
    "name": "@annebrown/vue3-fleet-pub",
    "description": "Fleet-Wide, Public, Vue 3 component Library",
    "version": "0.0.1",
    "private": false,
    "type": "module",
    "main": "./dist/vue3-fleet-pub.cjs",
    "module": "./dist/vue3-fleet-pub.mjs", 
    "unpkg": "./dist/vue3-fleet-pub.umd.cjs", 
    "exports": {
        ".": {
            "import": "./dist/vue3-fleet-pub.mjs",
            "require": "./dist/vue3-fleet-pub.cjs",
            "types": "./dist/index.d.ts" 
        }
    },

    "files": [
    "dist"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/annebrown/vue3-fleet-pub.git"
    },
    "keywords": [
        "vue",
        "vue3",
        "component",
        "ui",
        "fleet"
    ],
    "author": "Anne Brown <anne@browndomain.com>",
    "license": "MIT",
    "scripts": {
        "dev": "vite",
        "build": "vite build",
        "preview": "vite preview",
        "test:unit": "vitest",
        "lint": "eslint . --ext .vue,.js,.jsx,.mjs,.ts,.tsx,.cjs --fix --ignore-path .gitignore",
        "format": "prettier --write src/"
    },
    "dependencies": {
        "vue": "^3.4.21"
    },
        "devDependencies": {
        "@tsconfig/node22": "^22.0.2",
        "@vitejs/plugin-vue": "^5.0.0",
        "@vue/tsconfig": "^0.7.0",
        "vite": "^5.0.11",
        "vite-plugin-dts": "^4.5.4"
    }
}